{
  "name": "hexo-site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "hexo clean && npm run g && http-server",
    "g": "hexo clean && node --max_old_space_size=2048 ./node_modules/.bin/hexo g -c 1 --debug",
    "s": "hexo clean && node --max_old_space_size=2048 ./node_modules/.bin/hexo s --debug",
    "g-inspect": "hexo clean && node --max_old_space_size=2048 --inspect-brk=0.0.0.0:9229 ./node_modules/.bin/hexo g -c 1 --debug",
    "s-inspect": "hexo clean && node --max_old_space_size=2048 --inspect-brk=0.0.0.0:9229 ./node_modules/.bin/hexo s --debug"
  },
  "hexo": {
    "version": "3.9.0"
  },
  "dependencies": {
    "hexo": "^3.9.0",
    "hexo-generator-archive": "^0.1.4",
    "hexo-generator-category": "^0.1.3",
    "hexo-generator-index": "^0.2.0",
    "hexo-generator-tag": "^0.2.0",
    "hexo-renderer-ejs": "^0.3.0",
    "hexo-renderer-marked": "^0.3.0",
    "hexo-renderer-stylus": "^0.3.1",
    "hexo-server": "^1.0.0"
  },
  "devDependencies": {
    "http-server": "^0.11.1"
  }
}
